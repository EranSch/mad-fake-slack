{
  "name": "mad-fake-slack",
  "version": "1.0.0",
  "description": "Fake Slack implementation on node.js + express.js + express-ws",
  "main": "index.js",
  "jest": {
    "preset": "jest-puppeteer",
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/?(*.)+(spec|test).[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "scripts": {
    "test": "npm run test:integration",
    "test:jest": "jest",
    "test:integration": "npm run pretest && npx cucumber-js --fail-fast",
    "test:integration:only": "npm run pretest && npx cucumber-js --fail-fast --tags=@only",
    "start": "node index.js",
    "example:rtmbot": "SLACK_API=http://localhost:9001/api/ node examples/rtmbot/index.js",
    "example:rtmbot:integration": "npm run pretest && SLACK_API=http://0.0.0.0:9001/api/ npm run test:jest -- --runInBand examples/rtmbot/",
    "pretest": "eslint --ignore-path .gitignore .",
    "lint:hbs": "ember-template-lint views/* views/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maddevsio/mad-fake-slack.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maddevsio/mad-fake-slack/issues"
  },
  "homepage": "https://github.com/maddevsio/mad-fake-slack#readme",
  "dependencies": {
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-ws": "^4.0.0",
    "handlebars": "^4.1.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "@slack/rtm-api": "^5.0.1",
    "@slack/web-api": "^5.0.1",
    "bluebird": "^3.5.5",
    "cucumber": "^5.1.0",
    "ember-template-lint": "^1.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "jest": "^24.8.0",
    "jest-puppeteer": "^4.2.0",
    "node-fetch": "^2.6.0",
    "puppeteer": "^1.18.0"
  }
}
